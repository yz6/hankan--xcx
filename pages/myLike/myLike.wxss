/* pages/myLike/myLike.wxss */
.content{
  min-height: 100vh;
  width: 100%;
  background: #F2F2F2;
}
.noLikeLogoBox{
  padding-top: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.noLikeLogoBox image{
  width: 204rpx;
  height: 192rpx;
}
.contentBox{
  padding: 20rpx 0;
}
.cetner{
  display: flex;
  justify-content: center;
  margin: 20rpx 0;
  font-size: 28rpx;
  color: #999999;
}
.cardList{
  height: 536rpx;
  width: 326rpx;
  float: left;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
  background-color:#999999;
  border-radius: 24rpx;
  box-sizing: border-box;
  box-shadow: 0  8rpx 12rpx 0 rgba(0,0,0,.2);
}
.clearfloat::after{display:block;clear:both;content:"";visibility:hidden;height:0} 
  